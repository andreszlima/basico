<h1>People#new</h1>
<p>Find me in app/views/people/new.html.erb</p>



<%= form_for Person.new do |p| %>

  <%= p.label 'Nome:' %>
  <%= p.text_field :name %>

  <%= p.label 'Idade:' %>
  <%= p.number_field :age %>

  <br>

  <%= p.submit 'Criar pessoa' %>

<% end %>


